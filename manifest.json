{
  "manifest_version": 3,
  "name": "manaba+R Enhancer",
  "version": "3.1.0",
  "description": "manaba+Rのホーム画面に未提出課題一覧とカレンダーを追加します。",
  "permissions": [
    "storage",
    "downloads",
    "identity"
  ],
  "host_permissions": [
    "https://ct.ritsumei.ac.jp/*",
    "https://www.googleapis.com/*"
  ],
  "oauth2": {
    "client_id": "642837556502-t9fclukr83qls86g9f2uml98n9jdfc6s.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar.events"
    ]
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://ct.ritsumei.ac.jp/ct/home",
        "https://ct.ritsumei.ac.jp/ct/course*",
        "https://ct.ritsumei.ac.jp/ct/page*"
      ],
      "css": [
        "style.css",
        "vanilla-calendar.css"
      ],
      "js": [
        "vanilla-calendar.js",
        "content.js"
      ]
    }
  ]
}